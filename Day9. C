#include <stdio.h>
#include <string.h>

char* longestCommonPrefix(char strs[][100], int n) {
    static char prefix[100];
    int i, j;
    if (n == 0) return "";
    strcpy(prefix, strs[0]);
    for (i = 1; i < n; i++) {
        j = 0;
        while (prefix[j] && strs[i][j] && prefix[j] == strs[i][j]) j++;
        prefix[j] = '\0';
    }
    return prefix;
}

int main() {
    char strs[3][100] = {"flower","flow","flight"};
    int n = 3;
    printf("%s", longestCommonPrefix(strs, n));
    return 0;
}
