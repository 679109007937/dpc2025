#include <stdio.h>
#include <string.h>

int isValid(char *s) {
    char stack[1000];
    int top = -1;
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] == '(' || s[i] == '{' || s[i] == '[') {
            stack[++top] = s[i];
        } else {
            if (top == -1) return 0;
            if ((s[i] == ')' && stack[top] != '(') ||
                (s[i] == '}' && stack[top] != '{') ||
                (s[i] == ']' && stack[top] != '['))
                return 0;
            top--;
        }
    }
    return top == -1;
}

int main() {
    char s[100];
    scanf("%s", s);
    if (isValid(s))
        printf("true");
    else
        printf("false");
    return 0;
}
