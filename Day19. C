#include <stdio.h>
#include <string.h>
#include <ctype.h>
#include <stdlib.h>

int eval(char* exp) {
    int stack[100], top = -1;
    for (int i = 0; exp[i]; i++) {
        if (isdigit(exp[i])) {
            stack[++top] = exp[i] - '0';
        } else if (exp[i] == ' ') {
            continue;
        } else {
            int b = stack[top--];
            int a = stack[top--];
            switch (exp[i]) {
                case '+': stack[++top] = a + b; break;
                case '-': stack[++top] = a - b; break;
                case '*': stack[++top] = a * b; break;
                case '/': stack[++top] = a / b; break;
            }
        }
    }
    return stack[top];
}

int main() {
    char exp[] = "2 1 + 3 *";
    printf("%d", eval(exp));
    return 0;
}
