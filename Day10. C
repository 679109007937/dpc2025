#include <stdio.h>
#include <string.h>

void sortString(char *str) {
    int n = strlen(str);
    for (int i = 0; i < n-1; i++) {
        for (int j = i+1; j < n; j++) {
            if (str[i] > str[j]) {
                char temp = str[i];
                str[i] = str[j];
                str[j] = temp;
            }
        }
    }
}

int main() {
    char words[6][10] = {"eat", "tea", "tan", "ate", "nat", "bat"};
    int used[6] = {0};
    int n = 6;

    for (int i = 0; i < n; i++) {
        if (!used[i]) {
            char s1[10];
            strcpy(s1, words[i]);
            sortString(s1);
            printf("[ %s", words[i]);
            used[i] = 1;

            for (int j = i+1; j < n; j++) {
                char s2[10];
                strcpy(s2, words[j]);
                sortString(s2);
                if (strcmp(s1, s2) == 0) {
                    printf(" %s", words[j]);
                    used[j] = 1;
                }
            }
            printf(" ]\n");
        }
    }
    return 0;
}
