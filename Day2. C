#include <stdio.h>

int main() {
    int n, i, sum = 0, total;
    
    printf("Enter the value of n: ");
    scanf("%d", &n);
    
    int arr[n - 1];
    printf("Enter %d numbers: ", n - 1);
    for (i = 0; i < n - 1; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    
    total = n * (n + 1) / 2;  // Sum of first n natural numbers
    printf("Missing number is: %d\n", total - sum);
    
    return 0;
}
