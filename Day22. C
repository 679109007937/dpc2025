#include <stdio.h>

int main() {
    int arr[] = {3, 1, 4, 4, 5, 2, 6, 1, 4};
    int n = sizeof(arr) / sizeof(arr[0]);
    int k = 2;
    int i, j, count, found = -1;

    for (i = 0; i < n; i++) {
        count = 0;
        for (j = 0; j < n; j++) {
            if (arr[i] == arr[j]) {
                count++;
            }
        }
        if (count == k) {
            found = arr[i];
            break;
        }
    }

    printf("%d\n", found);
    return 0;
}
